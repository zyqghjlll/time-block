<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.calm.tools.repository.mysql.mapper.DbAgentMapper">

    <select id="findAll" resultType="com.calm.tools.botfy.mysql.entity.DbAgent">
        select * from db_agent;
    </select>
    <select id="findByCode" resultType="com.calm.tools.botfy.mysql.entity.DbAgent">
        select * from db_agent where code = #{code};
    </select>
    <select id="findByIp" resultType="com.calm.tools.botfy.mysql.entity.DbAgent">
        select * from db_agent where server_ip = #{ip};
    </select>
    <select id="findByGroupCode" resultType="com.calm.tools.botfy.mysql.entity.DbAgent">
        select * from db_agent where group_code = #{groupCode};
    </select>
    <select id="findByServer" resultType="com.calm.tools.botfy.mysql.entity.DbAgent">
        select * from db_agent where server_ip = #{ip} and server_port = #{port};
    </select>
</mapper>